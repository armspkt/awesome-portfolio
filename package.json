    "gsap": "^3.7.1",
    "isomorphic-unfetch": "^3.1.0",
    "lax.js": "^2.0.3",
    "next": "^11.1.2",
    "next-pwa": "^5.3.1",
    "next-seo": "^4.26.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-intersection-observer": "^8.32.1",
    "react-overdrive": "^0.0.10",
