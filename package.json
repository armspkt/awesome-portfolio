    "gsap": "^3.6.0",
    "isomorphic-unfetch": "^3.1.0",
    "lax.js": "^1.2.5",
    "next": "^10.0.1",
    "next-pwa": "^3.1.5",
    "next-seo": "^4.17.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-error-boundary": "^3.0.2",
    "react-intersection-observer": "^8.30.2",
    "react-overdrive": "^0.0.10",
