    "gsap": "^3.8.0",
    "isomorphic-unfetch": "^3.1.0",
    "lax.js": "^2.0.3",
    "next": "^12.1.5",
    "next-pwa": "^5.4.0",
    "next-seo": "^4.28.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-intersection-observer": "^8.32.2",
    "react-overdrive": "^0.0.10",
    "react-plx": "^1.3.17",
