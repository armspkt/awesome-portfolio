    "@types/react-transition-group": "^4.4.0",
    "@types/styled-components": "^5.1.4",
    "@types/styled-jsx": "^2.2.8",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "babel-plugin-styled-components": "^1.11.1",
