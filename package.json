  "devDependencies": {
    "@types/material-ui": "^0.21.12",
    "@types/next": "^9.0.0",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.23.0",
    "eslint-config-next": "^12.2.5",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": "^16.x"
  }
