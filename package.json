    "@types/react-transition-group": "^4.4.2",
    "@types/styled-components": "^5.1.14",
    "@types/styled-jsx": "^3.4.4",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "babel-plugin-styled-components": "^1.13.2",
